
@{
    ViewData["Title"] = "Index";
}

@model List<Employees>
<h1>Index</h1>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Birth Date</th>
        <th>Country</th>
    </tr>
    @foreach (Employees e in Model)
    {
        <tr>
            <td>@e.FirstName</td>
            <td>@e.LastName</td>
            <td>@e.BirthDate</td>
            <td>@e.Country</td>
        </tr>
    }
</table>

